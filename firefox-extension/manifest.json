{
  "manifest_version": 2,
  "name": "FindFirst",
  "version": "1.0",

  "description": "Saves links to FindFirst",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["ffextension.js"]
    }
  ],

  "icons": {
    "48": "icons/basic-f-v2-dark-mode-v2-fav-48.png",
    "96": "icons/basic-f-v2-dark-mode-v2-fav-96.png"
  }
}
